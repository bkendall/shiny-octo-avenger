{
  "parser": "babel-eslint",
  "plugins": ["babel"],
  "env": {
      "es6": true,
      "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [ 2, 2 ],
    "linebreak-style": [ 2, "unix" ],
    "max-len": [2, 80, 4],
    "quotes": [ 2, "single" ],
    "semi": [ 2, "always" ]
  }
}
