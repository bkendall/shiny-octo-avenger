{
  "name": "js-graph",
  "version": "0.0.0",
  "description": "graph database in javascript",
  "main": "index.js",
  "scripts": {
    "build": "babel src --ignore __tests__ --out-dir dist/",
    "docs": "jsdoc client/*",
    "integration": "mocha $npm_package_options_mocha src/client/**/__tests__/**/*.js",
    "lint": "eslint .",
    "preversion": ". ./resources/check-git.sh && npm test",
    "start": "node server/index.js",
    "start-watch": "nodemon --watch server server/index.js",
    "test": "npm run lint && npm run unit && npm run integration",
    "unit": "mocha $npm_package_options_mocha src/server/**/__tests__/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bkendall/shiny-octo-avenger.git"
  },
  "author": "Bryan Kendall <bryan@bryankendall.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bkendall/shiny-octo-avenger/issues"
  },
  "homepage": "https://github.com/bkendall/shiny-octo-avenger#readme",
  "options": {
    "mocha": "--require resources/mocha-bootload"
  },
  "dependencies": {
    "101": "^1.0.0",
    "babel-runtime": "^5.8.25",
    "body-parser": "^1.12.3",
    "boom": "^2.7.1",
    "dat-middleware": "^1.10.3",
    "express": "^4.12.3",
    "middlewarize": "0.0.1",
    "morgan": "^1.5.2",
    "simple-api-client": "^0.6.0",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "async": "^1.0.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "chai": "^3.3.0",
    "code": "^1.4.0",
    "eslint": "^1.3.1",
    "eslint-plugin-babel": "^2.1.1",
    "jsdoc": "^3.3.0",
    "lab": "^6.1.0",
    "mocha": "^2.3.3",
    "nodemon": "^1.3.7",
    "sinon": "^1.17.1"
  }
}
